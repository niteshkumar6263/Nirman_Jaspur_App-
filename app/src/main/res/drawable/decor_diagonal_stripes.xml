<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="600dp"
    android:height="600dp"
    android:viewportWidth="600"
    android:viewportHeight="600">

    <!-- repeated thin diagonal strokes (low alpha) -->
    <group android:rotation="-20" android:translateX="-200" android:translateY="-80">
        <path android:fillColor="#11000000" android:pathData="M0,0 L600,0 L600,6 L0,6z"/>
        <path android:fillColor="#11000000" android:pathData="M40,40 L640,40 L640,46 L40,46z"/>
        <path android:fillColor="#11000000" android:pathData="M80,80 L680,80 L680,86 L80,86z"/>
        <path android:fillColor="#0F000000" android:pathData="M120,120 L720,120 L720,126 L120,126z"/>
        <!-- You can add more paths for denser texture -->
    </group>
</vector>
